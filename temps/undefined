public class Worker {
  public String name; 
public int age ; 
public double wage ; 
  public Worker(String name, int age, double wage) {
  this.name = name ; 
this.age = age ; 
this.wage = wage ; 
}

  public String toString() {
  String newline = "\r\n"; 
   String msg = "name = " + name + newline 
       + "age = " + age + newline 
       + "wage = " + wage ; 

  // System.out.println(msg); 
return msg ; 
}
}
